{
  "name": "pkc",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "archive-ios": "(cd ios && xcodebuild archive -workspace pkc.xcworkspace -scheme pkc -archivePath ../output/myPKFiT.xcarchive)",
    "build": "yarn && yarn generate-app-env && yarn validate && yarn clean && react-native link && yarn generate-resources && yarn pod-install && jetify",
    "build-and-log": "yarn build &> build.log",
    "build-and-release:ios": "yarn build-ios && yarn archive-ios && yarn export-ipa",
    "build-demo": "ENV=demo yarn build",
    "build-demo2": "ENV=demo2 yarn build",
    "build-dev": "ENV=dev yarn build",
    "build-dev2": "ENV=dev2 yarn build",
    "build-ios": "(cd ios && xcodebuild build -workspace pkc.xcworkspace -scheme pkc)",
    "build-local": "ENV=local yarn build",
    "build-perf": "ENV=perf yarn build",
    "build-prd": "ENV=prd yarn build",
    "build-val": "ENV=val yarn build",
    "build-ver": "ENV=ver yarn build",
    "clean": "rimraf ios/build && rimraf ios/Pods && rimraf android/app/build",
    "coverage": "yarn test --verbose --coverage && open ./coverage/lcov-report/index.html",
    "coverage-pattern": "yarn test --verbose --coverage",
    "export-ipa": "(cd ios && xcodebuild -exportArchive -archivePath ../output/myPKFiT.xcarchive -exportPath ../output/ -exportOptionsPlist ExportOptions.plist)",
    "generate-app-env": "node scripts/build/generate-app-env.js",
    "generate-resources": "node scripts/build/generate-resources.js",
    "ios": "react-native run-ios",
    "lint": "tsc --noEmit && eslint --ext .tsx --ext .ts src/",
    "pod-install": "(cd ios && pod install --repo-update)",
    "prepare": "husky install",
    "release:android": "(cd android && ./gradlew assembleRelease)",
    "release:ios": "yarn archive-ios && yarn export-ipa",
    "start": "react-native start",
    "test": "TZ=UTC jest",
    "test:silent": "yarn test --silent",
    "test:watch": "yarn test --verbose --watch",
    "validate": "yarn validate-config",
    "validate-config": "node scripts/validate-config.js"
  },
  "dependencies": {
    "@miblanchard/react-native-slider": "^1.5.0",
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@react-native-community/datetimepicker": "^3.0.3",
    "@react-native-community/masked-view": "^0.1.9",
    "@react-native-community/netinfo": "^5.7.0",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-community/progress-bar-android": "^1.0.4",
    "@react-native-community/progress-view": "^1.2.3",
    "@react-native-firebase/analytics": "^10.8.0",
    "@react-native-firebase/app": "^10.8.0",
    "@react-native-firebase/crashlytics": "^10.8.0",
    "@react-navigation/bottom-tabs": "^5.10.4",
    "@react-navigation/native": "^5.8.4",
    "@react-navigation/stack": "^5.12.1",
    "blob-util": "^2.0.2",
    "crypto-es": "^1.2.6",
    "dotenv": "^8.2.0",
    "events": "^3.1.0",
    "generator-rn-toolbox": "vdelacou/generator-rn-toolbox.git#d10da2d",
    "i18next": "^19.4.0",
    "moment": "^2.24.0",
    "moment-recur": "^1.0.7",
    "moment-recur-ts": "^1.3.1",
    "moment-timezone": "^0.5.31",
    "react": "16.13.1",
    "react-i18next": "^11.3.4",
    "react-native": "0.63.3",
    "react-native-action-sheet": "^2.2.0",
    "react-native-autoheight-webview": "^1.5.1",
    "react-native-background-timer-android": "mimacom/react-native-background-timer-android#e95bd1d",
    "react-native-calendars": "mimacom/react-native-calendars#dist",
    "react-native-camera": "^3.30.0",
    "react-native-device-info": "^8.0.1",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-google-fit": "^0.16.2",
    "react-native-image-picker": "^3.8.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-keychain": "mimacom/react-native-keychain#dist",
    "react-native-localize": "^2.2.1",
    "react-native-logs": "^3.0.3",
    "react-native-mlkit-ocr": "^0.2.2",
    "react-native-modal": "^11.5.6",
    "react-native-modal-datetime-picker": "^9.0.0",
    "react-native-pdf": "^6.2.2",
    "react-native-permissions": "^2.1.5",
    "react-native-picker-select": "^8.0.2",
    "react-native-popover-view": "^3.0.3",
    "react-native-push-notification": "^5.1.0",
    "react-native-qrcode-scanner": "^1.4.1",
    "react-native-reanimated": "^1.8.0",
    "react-native-render-html": "^4.2.3",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.4.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^9.13.3",
    "react-native-svg-charts": "^5.3.0",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-swipe-list-view": "^3.2.3",
    "react-native-swiper": "^1.6.0",
    "react-native-touch-id": "^4.3.0",
    "react-native-user-inactivity": "^1.1.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-video": "^5.1.1",
    "react-native-webview": "^10.3.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "rn-apple-healthkit": "mimacom/rn-apple-healthkit#develop",
    "rn-fetch-blob": "^0.12.0",
    "rn-snackbar-component": "^0.0.4",
    "rxjs": "^6.5.5",
    "yo": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@types/jest": "^23.3.5",
    "@types/moment-timezone": "^0.5.13",
    "@types/react": "^16.8.22",
    "@types/react-native": "^0.63.26",
    "@types/react-native-calendars": "^1.20.4",
    "@types/react-native-push-notification": "^5.0.1",
    "@types/react-native-svg-charts": "^5.0.1",
    "@types/react-native-vector-icons": "^4.6.4",
    "@types/react-native-video": "^5.0.4",
    "@types/react-redux": "^7.1.2",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-header": "^3.1.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-react-native": "^3.10.0",
    "fs-extra": "^9.0.0",
    "husky": "^6.0.0",
    "jest": "^25.1.0",
    "jetifier": "^1.6.4",
    "jsonschema": "^1.3.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "2.0.2",
    "react-devtools": "^4.7.0",
    "react-native-performance": "^1.4.0",
    "react-native-typescript-transformer": "^1.2.10",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "ts-json-schema-generator": "^0.77.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/*.constants.*",
      "!**/*.mocks.*",
      "!**/__mocks__",
      "!**/index.d.ts",
      "!src/services/tests/test.domain.ts"
    ],
    "coverageReporters": [
      "text",
      "cobertura",
      "lcov",
      "clover"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/?!(react-native-root-siblings)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "resolutions": {
    "**/moment": "2.27.0",
    "graceful-fs": "4.2.4"
  },
  "rnpm": {
    "assets": [
      "./resources/fonts/mmcIcons/fonts/"
    ]
  }
}
